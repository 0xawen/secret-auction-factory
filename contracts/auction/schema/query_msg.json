{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "description": "Queries",
  "anyOf": [
    {
      "description": "Displays the auction information",
      "type": "object",
      "required": [
        "auction_info"
      ],
      "properties": {
        "auction_info": {
          "type": "object"
        }
      }
    },
    {
      "description": "View active bid for input address",
      "type": "object",
      "required": [
        "view_bid"
      ],
      "properties": {
        "view_bid": {
          "type": "object",
          "required": [
            "address",
            "viewing_key"
          ],
          "properties": {
            "address": {
              "$ref": "#/definitions/HumanAddr"
            },
            "viewing_key": {
              "type": "string"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "HumanAddr": {
      "type": "string"
    }
  }
}
